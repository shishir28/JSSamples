<div class="weatherWidget">
    {{#each partials.weather.locations}}
    <div class="locations">
        <h3>{{name}}</h3>
        <a href="{{forcasturl}}">
            <img src="={{iconUrl}}" alt="{{weather}}">{{weather}},{{temp}}
        </a>
    </div>
    {{/each}}
    <small> Source:<a href="https://www.wunderground.com">Weather Underground</a> </small>
</div>